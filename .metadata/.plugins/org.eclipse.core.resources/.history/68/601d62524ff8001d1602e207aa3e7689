package test;

public class Test {

	public static void main(String[] args) {
		// 글 문자열 
		// String 
		// 기본 데이터 타입, 참조 데이터 타입
		String str = "hello";
		String world = "world";
		System.out.println(str + " " + world);
		String a = "a";
		// char 1 글자
		Character b = 'b';
		System.out.println(a + b);
		
	}

}
