package test;

public class Main {

	public static void main(String[] args) {
		// 변수  
		int a = 127;
		int b = 22;
		a = a + 1;
		// 변수 선언
		// 숫자 
		// byte -> short -> int -> long  
		// 2^8-1 -> 2^16-1 -> 2^32-1 -> 2^64-1
		// + - * / 
		// 실수 
		// float -> double
		// 2^32 -> 2^64
		System.out.println(a / b); // 5 
	}

}
