
public class Test {

	public static void main(String[] args) {
		int[][] arr = {
				{1,2,3},
				{4,5,6},
				{7,8,9}
				}; 
		
		// 987654321
		for (int i=arr.length; i>0; i--) {
			for (int j=arr[i-1].length; j>0; j--) {
				System.out.println(arr[i-1][j-1]);
			}
		}
		// 147258369
		for(int i=0; i<arr.length; i++) { 
			// arr.length =3 0,1,2
			for(int j=0; j<arr.length; j++) {
				// arr.length = 3 0,1,2
				System.out.println(arr[j][i]);
			}
		}
	}

}
