package unit;

import data.Unit;

public class Zealout extends Unit{
	private int shield = 100;
	
	public Zealout() {
		super("Zealout", 100, 12);
	}
	@Override
	public String getDamage(Unit unit) {
		this.hp = this.hp - unit.getAttackPoint();
		return name+ "가 "+ unit.getName()+ "한테 "
				+ unit.getAttackPoint() + "딜을 받았다";
	}
}
