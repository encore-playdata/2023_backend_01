package test;

public class Star {

	public static void main(String[] args) {
		  for (int i = 1; i < 6; i++) {
	            for (int j = 0; j < i; j++) {
	                System.out.print("*");
	            }
	            System.out.println("");
	        }
	    String star = "";
		for(int i=1; i<6; i++ ) {
			star += "*";
			System.out.println(star);
		}
	}
}
